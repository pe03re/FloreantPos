<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
	<class lazy="false" name="com.floreantpos.model.ExpenseTransaction"
		table="EXPENSE_TRANSACTION">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>


		<property column="TRANSACTION_DATE" name="transactionDate"
			type="java.util.Date" />
		<property column="AMOUNT" name="amount" type="java.lang.Double" />
		<property column="VAT_PAID" name="vatPaid" type="java.lang.Double" />
		<property column="CREDIT_CHECK" name="creditCheck" type="java.lang.Boolean" />
		<property column="REMARK" name="remark" type="string" length="255" />

		<many-to-one class="ExpenseTransactionType" name="transactionType"
			column="TRANSACTION_TYPE_ID" />
		<many-to-one class="PurchaseOrder" name="referenceNo"
			column="REFERENCE_ID" />
		<many-to-one class="InventoryVendor" name="vendor" column="VENDOR_ID" />

	</class>
</hibernate-mapping>