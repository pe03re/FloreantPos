<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
	<class lazy="false" name="com.floreantpos.model.ExpenseTransaction"
		table="EXPENSE_TRANSACTION">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="identity" />
		</id>


		<many-to-one name="expenseTransactionType" class="com.test.ExpenseTransactionType"
			fetch="select">
			<column name="TRANSACTION_TYPE_ID" />
		</many-to-one>
		<many-to-one name="inventoryVendor" class="com.test.InventoryVendor"
			fetch="select">
			<column name="VENDOR_ID" />
		</many-to-one>
		<many-to-one name="expenseHead" class="com.test.ExpenseHead"
			fetch="select">
			<column name="EXPENSE_HEAD_ID" />
		</many-to-one>
		<property name="transactionDate" type="timestamp">
			<column name="TRANSACTION_DATE" length="19" />
		</property>
		<property name="amount" type="java.lang.Double">
			<column name="AMOUNT" precision="22" scale="0" />
		</property>
		<property name="vatPaid" type="java.lang.Double">
			<column name="VAT_PAID" precision="22" scale="0" />
		</property>
		<property name="creditCheck" type="boolean">
			<column name="CREDIT_CHECK" not-null="true" />
		</property>
		<property name="remark" type="string">
			<column name="REMARK" />
		</property>

	</class>
</hibernate-mapping>