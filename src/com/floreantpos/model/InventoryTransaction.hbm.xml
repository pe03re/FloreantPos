<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
  <class lazy="false" name="com.floreantpos.model.InventoryTransaction"  table="INVENTORY_TRANSACTION">
    <id column="ID" name="id" type="java.lang.Integer">
      <generator class="identity"/>
    </id>
    
    
    <many-to-one name="inventoryTransaction" class="com.test.InventoryTransaction" fetch="select">
            <column name="TRANSACTION_TYPE_ID" not-null="true" />
        </many-to-one>
        <many-to-one name="inventoryWarehouseByFromWarehouseId" class="com.test.InventoryWarehouse" fetch="select">
            <column name="FROM_WAREHOUSE_ID" />
        </many-to-one>
        <many-to-one name="inventoryWarehouseByToWarehouseId" class="com.test.InventoryWarehouse" fetch="select">
            <column name="TO_WAREHOUSE_ID" />
        </many-to-one>
        <many-to-one name="inventoryItem" class="com.test.InventoryItem" fetch="select">
            <column name="ITEM_ID" not-null="true" />
        </many-to-one>
        <many-to-one name="company" class="com.test.Company" fetch="select">
            <column name="COMPANY_ID" not-null="true" />
        </many-to-one>
        <many-to-one name="packSize" class="com.test.PackSize" fetch="select">
            <column name="SIZE_ID" not-null="true" />
        </many-to-one>
        <many-to-one name="inventoryVendor" class="com.test.InventoryVendor" fetch="select">
            <column name="VENDOR_ID" not-null="true" />
        </many-to-one>
        <property name="transactionDate" type="timestamp">
            <column name="TRANSACTION_DATE" length="19" not-null="true" />
        </property>
        <property name="quantity" type="double">
            <column name="QUANTITY" precision="22" scale="0" not-null="true" />
        </property>
        <property name="totalPrice" type="double">
            <column name="TOTAL_PRICE" precision="22" scale="0" not-null="true" />
        </property>
        <property name="vatPaid" type="double">
            <column name="VAT_PAID" precision="22" scale="0" not-null="true" />
        </property>
        <property name="creditCheck" type="boolean">
            <column name="CREDIT_CHECK" not-null="true" />
        </property>
        <property name="remark" type="string">
            <column name="REMARK" />
        </property>
        <property name="discount" type="double">
            <column name="DISCOUNT" precision="22" scale="0" not-null="true" />
        </property>
        <set name="inventoryTransactions" table="inventory_transaction" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="TRANSACTION_TYPE_ID" not-null="true" />
            </key>
            <one-to-many class="com.test.InventoryTransaction" />
        </set>
    
  </class>
</hibernate-mapping>