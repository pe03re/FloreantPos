<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.floreantpos.model">
  <class lazy="false" name="com.floreantpos.model.InventoryVendor"  table="INVENTORY_VENDOR">
    <id column="ID" name="id" type="java.lang.Integer">
      <generator class="identity"/>
    </id>
    
    
   <many-to-one name="personByP2Id" class="com.floreantpos.model.Person" fetch="select">
            <column name="P2_ID" />
        </many-to-one>
        <many-to-one name="personByP5Id" class="com.floreantpos.model.Person" fetch="select">
            <column name="P5_ID" />
        </many-to-one>
        <many-to-one name="personByP4Id" class="com.floreantpos.model.Person" fetch="select">
            <column name="P4_ID" />
        </many-to-one>
        <many-to-one name="personByP6Id" class="com.floreantpos.model.Person" fetch="select">
            <column name="P6_ID" />
        </many-to-one>
        <many-to-one name="personByP3Id" class="com.floreantpos.model.Person" fetch="select">
            <column name="P3_ID" />
        </many-to-one>
        <many-to-one name="personByP7Id" class="com.floreantpos.model.Person" fetch="select">
            <column name="P7_ID" />
        </many-to-one>
        <property name="name" type="string">
            <column name="NAME" length="60" not-null="true" />
        </property>
        <property name="phone" type="string">
            <column name="PHONE" length="100" />
        </property>
        <property name="email" type="string">
            <column name="EMAIL" length="30" />
        </property>
        <property name="address" type="string">
            <column name="ADDRESS" length="250" />
        </property>
        <property name="visible" type="java.lang.Boolean">
            <column name="VISIBLE" />
        </property>
        <property name="expenseTypeVendor" type="boolean">
            <column name="EXPENSE_TYPE_VENDOR" not-null="true" />
        </property>
        <set name="expenseTransactions" table="expense_transaction" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="VENDOR_ID" />
            </key>
            <one-to-many class="com.floreantpos.model.ExpenseTransaction" />
        </set>
        <set name="inventoryTransactions" table="inventory_transaction" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="VENDOR_ID" not-null="true" />
            </key>
            <one-to-many class="com.floreantpos.model.InventoryTransaction" />
        </set>
        <set name="itemCompVendPacks" table="item_comp_vend_pack" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="VENDOR_ID" not-null="true" />
            </key>
            <one-to-many class="com.floreantpos.model.ItemCompVendPack" />
        </set>
    
  </class>
</hibernate-mapping>